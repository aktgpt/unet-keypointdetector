{
    "data": {
        "image_folder":         "",
        "annotation_folder":    "",
        "augment":              true,
        "bbox_scale_factor":     0.3,
        "negative_sample_ratio": 0.3
    },

    "mask": {
        "kernel_size":          5,
        "variance":             0.3
    },

    "train": {
        "learning_rate":        1e-5,
        "decay_type":           "step",
        "learning_rate_decay":  0.1,
        "epoch_decay":          10,
        "epochs":               100,
        "batch_size":           8,
        "val_split_ratio":      0.2,
        "model_save_path":      "",
        "dropout":              0.2,
        "log_dir":              ""
    },

    "unet": {
        "input_shape":          [128, 128, 3],
        "n_classes":            1,
        "start_ch":             32,
        "depth":                5,
        "batch_norm":           false,
        "up_mode":              "transpose",
        "merge_mode":           "add"
    },

}
